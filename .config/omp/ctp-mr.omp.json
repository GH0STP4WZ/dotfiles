{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#1E1E2E",
          "foreground": "#F5E0DC",
          "style": "diamond",
          "template": " {{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#302D41",
          "foreground": "#F5E0DC",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "folder_icon": "...",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#6E6C7E",
          "foreground": "#F5E0DC",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#F38BA8",
          "foreground": "#F5E0DC",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#F5C2E7",
          "foreground": "#1E1E2E",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#F38BA8{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \uf08a{{ if gt .Code 0 }} {{.Code}}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#1E1E2E",
          "foreground": "#F5E0DC",
          "style": "plain",
          "properties": {
            "time_format": "03:04 PM"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#302D41",
          "foreground": "#FAB387",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#302D41",
          "foreground": "#A6E3A1",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "go"
        },
        {
          "background": "#302D41",
          "foreground": "#94E2D5",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "background": "#302D41",
          "foreground": "#F38BA8",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "ruby"
        },
        {
          "background": "#302D41",
          "foreground": "#F38BA8",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": " \ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "java"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#302D41",
          "foreground": "#F5E0DC",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "\ue0b4",
          "type": "session"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "final_space": true,
  "version": 3
}